    "@types/yargs": "^17.0.9",
    "fs": "0.0.1-security",
    "gaxios": "^4.3.2",
    "gcf-utils": "^11.4.3",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "nodejs-fetch": "^1.0.0",
    "snap-shot-it": "^7.9.6",
    "uuidv4": "^6.2.12",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/jwt-encode": "^1.0.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.0",
