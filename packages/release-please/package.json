{
  "name": "release-please-bot",
  "version": "1.2.0",
  "description": "propose releases",
  "private": true,
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "lint"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "node ./build/src/server.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --node-option no-experimental-fetch --exit build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-automations/bot-config-utils": "^8.0.0",
    "@google-automations/datastore-lock": "^6.0.0",
    "@google-automations/issue-utils": "^4.0.0",
    "@google-automations/label-utils": "^5.0.0",
    "@octokit/rest": "^19.0.4",
    "@octokit/webhooks": "^10.1.5",
    "gcf-utils": "^16.1.0",
    "jsonwebtoken": "^9.0.0",
    "release-please": "^16.14.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.15",
    "@types/semver": "^7.3.12",
    "@types/sinon": "^10.0.13",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "gts": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lru-cache": "^7.14.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "sinon": "^18.0.0",
    "snap-shot-it": "^7.9.6",
    "typescript": "~4.8.2"
  },
  "engines": {
    "node": ">= 18"
  }
}
