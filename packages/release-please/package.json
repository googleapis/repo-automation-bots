  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.2",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^10.0.6",
    "c8": "^7.10.0",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "js-yaml": "^4.1.0",
