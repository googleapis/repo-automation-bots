{
  "name": "release-please-bot",
  "version": "1.2.0",
  "description": "propose releases",
  "private": true,
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "lint"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/release-please.js",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --timeout=15000 --recursive ./build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "gcf-utils": "1.6.2",
    "release-please": "^3.2.3"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/ioredis": "^4.0.13",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^7.0.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.6.1",
    "@types/semver": "^7.0.0",
    "c8": "^7.1.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "gts": "^1.0.0",
    "mocha": "^7.0.0",
    "nock": "^12.0.0",
    "snap-shot-it": "^7.8.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
