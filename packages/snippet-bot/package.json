{
  "name": "snippet-bot",
  "version": "1.0.0",
  "description": "checks snippets for errors",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "files": [
    "build/src"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/snippet-bot.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --exit build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-automations/bot-config-utils": "^4.0.0",
    "@google-automations/label-utils": "^2.0.0",
    "@google-cloud/storage": "^5.8.5",
    "gcf-utils": "^12.0.0",
    "minimatch": "^3.0.4",
    "node-fetch": "^2.6.1",
    "parse-diff": "^0.8.1",
    "tar": "^6.1.0",
    "tmp-promise": "^3.0.2"
  },
  "devDependencies": {
    "@types/minimatch": "^3.0.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.3.3",
    "@types/node-fetch": "^2.5.11",
    "@types/sinon": "^10.0.2",
    "@types/tar": "^6.0.0",
    "@types/tmp": "^0.2.1",
    "@octokit/webhooks-definitions": "^3.67.3",
    "c8": "^7.7.3",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "mocha": "^9.0.2",
    "nock": "^13.1.1",
    "sinon": "^12.0.0",
    "smee-client": "^1.2.2",
    "snap-shot-it": "^7.9.6",
    "typescript": "~4.5.0"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
