{
  "name": "snippet-bot",
  "version": "1.0.0",
  "description": "checks snippets for errors",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "files": [
    "build/src"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "node ./build/src/server.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --exit build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-automations/bot-config-utils": "^6.1.0",
    "@google-automations/label-utils": "^3.0.0",
    "@google-cloud/storage": "^6.4.1",
    "follow-redirects": "^1.15.1",
    "gcf-utils": "^14.0.1",
    "lru-cache": "^7.14.0",
    "minimatch": "^5.1.0",
    "parse-diff": "^0.9.0",
    "tar": "^6.1.11",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^6.3.6",
    "@types/follow-redirects": "^1.14.1",
    "@types/minimatch": "^5.1.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.13",
    "@types/sinon": "^10.0.13",
    "@types/tar": "^6.1.2",
    "@types/tmp": "^0.2.3",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "gts": "^4.0.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "sinon": "^14.0.0",
    "smee-client": "^1.2.3",
    "snap-shot-it": "^7.9.6",
    "typescript": "~4.7.4"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "engines": {
    "node": ">= 14"
  }
}
