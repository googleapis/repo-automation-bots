{
  "name": "snippet-bot",
  "version": "1.0.0",
  "description": "checks snippets for errors",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "files": [
    "build/src"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/snippet-bot.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --exit build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-automations/bot-config-utils": "^3.0.0",
    "@google-automations/label-utils": "^1.0.0",
    "@google-cloud/storage": "^5.4.0",
    "gcf-utils": "^7.2.1",
    "minimatch": "^3.0.4",
    "node-fetch": "^2.6.0",
    "parse-diff": "^0.8.0",
    "tar": "^6.0.5",
    "tmp-promise": "^3.0.2"
  },
  "devDependencies": {
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.6.1",
    "@types/node-fetch": "^2.5.7",
    "@types/sinon": "^9.0.8",
    "@types/tar": "^4.0.3",
    "@types/tmp": "^0.2.0",
    "@octokit/webhooks-definitions": "^3.67.3",
    "c8": "^7.1.0",
    "cross-env": "^7.0.0",
    "gts": "^3.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "sinon": "^10.0.0",
    "smee-client": "^1.1.0",
    "snap-shot-it": "^7.8.0",
    "typescript": "~4.2.0"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
