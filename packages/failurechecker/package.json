{
  "name": "failurechecker",
  "version": "1.2.0",
  "description": "opens issues warning about releases that failed to complete",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/failurechecker.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-automations/issue-utils": "^1.0.0",
    "gcf-utils": "^14.0.0"
  },
  "devDependencies": {
    "@octokit/types": "^6.35.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/sinon": "^10.0.11",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.7",
    "sinon": "^14.0.0",
    "smee-client": "^1.2.3",
    "snap-shot-it": "^7.9.6",
    "typescript": "~4.7.4"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
