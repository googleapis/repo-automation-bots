{
  "name": "failurechecker",
  "version": "1.2.0",
  "description": "opens issues warning about releases that failed to complete",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/failurechecker.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "gcf-utils": "^13.0.2"
  },
  "devDependencies": {
    "@octokit/types": "^6.31.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "@types/sinon": "^10.0.4",
    "c8": "^7.9.0",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "mocha": "^9.1.2",
    "nock": "^13.1.3",
    "sinon": "^11.1.2",
    "smee-client": "^1.2.2",
    "snap-shot-it": "^7.9.6",
    "typescript": "~4.4.3"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
