{
  "name": "sync-repo-settings",
  "version": "0.0.1",
  "description": "Synchronize repository settings against a common config.",
  "private": true,
  "author": "Google LLC",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "compile": "tsc -p . && cp ./src/required-checks.json ./build/src/required-checks.json.json && cp ./src/teams.json ./build/src/teams.json.json",
    "start": "probot run ./build/src/sync-repo-settings",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "gaxios": "^3.0.0",
    "gcf-utils": "^5.0.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.4",
    "@types/express": "^4.17.1",
    "@types/ioredis": "^4.0.18",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.6",
    "c8": "^7.1.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "gts": "^2.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "snap-shot-it": "^7.9.0",
    "typescript": "^3.8.2"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
