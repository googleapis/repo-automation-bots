{
  "name": "publishbot",
  "version": "1.1.0",
  "description": "publis packages to package registries, when releases are created",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/publish.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.2.0",
    "gcf-utils": "^5.3.0",
    "node-fetch": "^2.6.1",
    "tar": "^6.0.5",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/node-fetch": "^2.5.7",
    "@types/tar": "^4.0.3",
    "@types/uuid": "^8.3.0",
    "c8": "^7.3.3",
    "cross-env": "^7.0.2",
    "gts": "^2.0.0",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "smee-client": "^1.2.2",
    "snap-shot-it": "^7.9.3",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
