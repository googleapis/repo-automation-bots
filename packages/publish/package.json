  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.7",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/ioredis": "^4.0.13",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^8.0.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.6.1",
    "@types/node-fetch": "^2.5.4",
    "@types/tar": "^4.0.3",
