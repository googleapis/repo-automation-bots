{
  "name": "slo-status-label",
  "version": "1.0.0",
  "description": "A bot that updates the label on issues and pr if it is OOSLO with out-of-SLO label",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "files": [
    "build/src"
  ],
  "scripts": {
    "compile": "tsc -p . && cp ./utils/schema.json ./build/utils/schema.json",
    "start": "probot run ./build/src/slo-status-label",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@octokit/rest": "^16.41.2",
    "@types/chai-spies": "^1.0.1",
    "ajv": "^6.0.0",
    "chai-spies": "^1.0.0",
    "gcf-utils": "1.6.2",
    "probot": "^9.11.3",
    "sinon": "^9.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/ioredis": "^4.0.13",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^7.0.1",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.6.1",
    "body-parser": "^1.19.0",
    "c8": "^7.1.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.0.0",
    "gts": "^2.0.0",
    "mocha": "^7.0.1",
    "nock": "^12.0.0",
    "smee-client": "^1.1.0",
    "snap-shot-it": "^7.8.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
