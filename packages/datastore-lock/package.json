{
  "name": "@google-automations/datastore-lock",
  "version": "7.1.0",
  "description": "Distributed lock backed by Google Cloud Datastore",
  "scripts": {
    "compile": "tsc -p .",
    "pretest": "npm run compile && docker pull google/cloud-sdk:510.0.0",
    "prepare": "npm run compile",
    "test": "cross-env NODE_ENV=test LOG_LEVEL=fatal c8 mocha --node-option no-experimental-fetch ./build/test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "main": "./build/src/datastore-lock.js",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "dependencies": {
    "@google-cloud/datastore": "^9.0.0",
    "gcf-utils": "^17.0.0",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.15",
    "@types/sinon": "^10.0.13",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "google-datastore-emulator": "^6.0.0",
    "gts": "^4.0.0",
    "lru-cache": "^7.14.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "sinon": "^15.0.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "google cloud datastore",
    "lock"
  ],
  "author": "Takashi Matsuo <tmatsuo@google.com>",
