{
  "name": "slo-bot",
  "version": "0.0.1",
  "description": "Labels SLO status on Issues and PRs",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "slo",
    "probot",
    "github",
    "probot-app"
  ],
  "files": [
    "build/src",
    "build/data"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/slo-bot.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --exit build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "ajv": "^7.0.0",
    "gcf-utils": "^7.1.1",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.6",
    "@probot/octokit-plugin-config": "^1.0.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.6.1",
    "@types/sinon": "^9.0.5",
    "c8": "^7.1.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.0.0",
    "gts": "^3.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "sinon": "^9.0.2",
    "smee-client": "^1.1.0",
    "snap-shot-it": "^7.8.0",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
