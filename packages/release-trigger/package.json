{
  "name": "release-trigger",
  "version": "1.0.0",
  "description": "Trigger kokoro release jobs",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "files": [
    "build/src"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "node ./build/src/server.js",
    "start-frontend": "node ./build/src/server-frontend.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --node-option no-experimental-fetch --exit build/test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-automations/bot-config-utils": "^9.0.0",
    "@google-automations/datastore-lock": "^7.0.0",
    "@google-automations/issue-utils": "^5.0.0",
    "gcf-utils": "^17.1.2",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.0.0",
    "@types/sinon": "^10.0.13",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "formidable": ">=2.1.3",
    "gts": "^4.0.0",
    "lru-cache": "^7.14.0",
    "mocha": "^10.0.0",
    "node-fetch": "^2.6.0",
    "nock": "^13.2.9",
    "sinon": "^15.0.0",
    "smee-client": "^1.2.3",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">= 18"
  },
  "overrides": {
    "tmp": "0.2.5"
  }
