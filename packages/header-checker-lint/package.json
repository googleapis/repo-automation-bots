{
  "name": "header-checker-lint",
  "version": "1.2.0",
  "description": "lint commit messages based on conventionalcommits.org",
  "private": true,
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "lint"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/header-checker-lint.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "gcf-utils": "^7.2.2",
    "@google-automations/bot-config-utils": "^3.0.0",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@octokit/rest": "^18.5.3",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.6.1",
    "@types/sinon": "^9.0.8",
    "c8": "^7.1.0",
    "cross-env": "^7.0.0",
    "gts": "^3.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "sinon": "^10.0.0",
    "snap-shot-it": "^7.8.0",
    "typescript": "~4.2.0"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
