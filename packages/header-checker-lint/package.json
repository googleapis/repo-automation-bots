{
  "name": "header-checker-lint",
  "version": "1.2.0",
  "description": "lint commit messages based on conventionalcommits.org",
  "private": true,
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src",
    "src/content"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "lint"
  ],
  "scripts": {
    "compile": "tsc -p . && cp -r src/content build/src/",
    "start": "probot run ./build/src/header-checker-lint.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@octokit/types": "^5.5.0",
    "code-suggester": "^1.1.0",
    "gcf-utils": "^6.1.1",
    "minimatch": "^3.0.4",
    "mustache": "^4.0.1"
  },
  "devDependencies": {
    "@probot/octokit-plugin-config": "^1.0.0",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^8.0.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.6.1",
    "c8": "^7.1.0",
    "cross-env": "^7.0.0",
    "gts": "^2.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "snap-shot-it": "^7.8.0",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
