{
  "report-dir": "coverage",
  "reporter": ["text", "lcov"],
  "exclude": [
    "**/src/bin/**",
    "**/*-test",
    "**/coverage",
    "**/apis",
    "**/benchmark",
    "**/conformance",
    "**/docs",
    "**/samples",
    "**/scripts",
    "**/protos",
    "**/test",
    "**/*.d.ts",
    ".jsdoc.js",
    "**/.jsdoc.js",
    "karma.conf.js",
    "webpack-tests.config.js",
    "webpack.config.js",
    ".prettierrc.js",
    ".mocharc.js"
  ],
  "exclude-after-remap": false,
  "all": true
}
