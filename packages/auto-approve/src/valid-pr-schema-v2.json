{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "@google-automations/auto-approve/v2",
  "description": "A PR safe to merge",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "processes": {
      "type": "array",
      "uniqueItems": true,
      "minItems": 1,
      "items": {
        "type": "string",
        "enum": [
          "UpdateDiscoveryArtifacts",
          "RegenerateReadme",
          "DiscoveryDocUpdate",
          "PythonDependency",
          "PythonSampleDependency",
          "NodeDependency",
          "NodeRelease",
          "JavaApiaryCodegen",
          "JavaDependency",
          "OwlBotTemplateChanges",
          "OwlBotAPIChanges",
          "PHPApiaryCodegen",
          "PythonSampleAppDependency",
          "JavaSampleAppDependency",
          "DockerDependency",
          "GoDependency",
          "GoApiaryCodegen",
          "NodeGeneratorDependency"
        ]
      }
    }
  }
 }
 
