    "@probot/octokit-plugin-config": "^1.1.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@types/sinon": "^10.0.4",
    "c8": "^7.10.0",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "js-yaml": "^4.1.0",
    "mocha": "^9.1.3",
