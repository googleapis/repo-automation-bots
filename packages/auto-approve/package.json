  "devDependencies": {
    "@octokit/rest": "^18.6.7",
    "@octokit/webhooks": "^9.10.0",
    "@octokit/webhooks-definitions": "^3.62.6",
    "@probot/octokit-plugin-config": "^1.1.1",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.3.1",
    "@types/sinon": "^10.0.2",
    "c8": "^7.7.3",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "js-yaml": "^4.1.0",
    "mocha": "^9.0.2",
    "nock": "^13.1.1",
