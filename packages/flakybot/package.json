{
  "name": "flakybot",
  "version": "1.1.0",
  "description": "creates issues on GitHub for failed kokoro test cases",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "node ./build/src/server.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --node-option no-experimental-fetch build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-automations/bot-config-utils": "^9.0.0",
    "@google-automations/datastore-lock": "^7.0.0",
    "@google-automations/label-utils": "^6.0.0",
    "@octokit/openapi-types": "^14.0.0",
    "@octokit/types": "^8.0.0",
    "gcf-utils": "^17.0.0",
    "jsonwebtoken": "^9.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.15",
    "@types/sinon": "^10.0.13",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "gts": "^4.0.0",
    "lru-cache": "^7.14.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "node-fetch": "^2.6.9",
    "sinon": "^15.0.0",
    "snap-shot-it": "^7.9.6",
    "typescript": "~4.9.0"
  },
  "engines": {
    "node": ">= 18"
  }
}
