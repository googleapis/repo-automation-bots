{
  "name": "flakybot",
  "version": "1.1.0",
  "description": "creates issues on GitHub for failed kokoro test cases",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "node ./build/src/main.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-automations/bot-config-utils": "^3.1.1",
    "@google-automations/datastore-lock": "^2.0.1",
    "@octokit/openapi-types": "^7.0.0",
    "@octokit/types": "^6.14.2",
    "probot": "^11.3.0",
    "gcf-utils": "^8.0.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.0",
    "@types/sinon": "^10.0.0",
    "c8": "^7.1.0",
    "cross-env": "^7.0.0",
    "gts": "^3.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "sinon": "^11.0.0",
    "snap-shot-it": "^7.8.0",
    "typescript": "~4.3.0"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
