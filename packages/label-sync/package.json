    "test": "cross-env LOG_LEVEL=fatal c8 mocha build/test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.0.0",
    "gcf-utils": "^5.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.6",
    "@types/sinon": "^9.0.4",
    "c8": "^7.1.0",
    "cross-env": "^7.0.0",
    "gts": "^2.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "sinon": "^9.0.2",
    "snap-shot-it": "^7.9.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
