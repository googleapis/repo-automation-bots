  "dependencies": {
    "@google-cloud/storage": "^5.19.4",
    "gaxios": "^5.0.0",
    "gcf-utils": "^13.5.2"
  },
  "devDependencies": {
    "@probot/octokit-plugin-config": "^1.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@types/sinon": "^10.0.11",
    "c8": "^7.11.2",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.4",
    "sinon": "^13.0.2",
    "snap-shot-it": "^7.9.6",
    "typescript": "~4.6.4"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
