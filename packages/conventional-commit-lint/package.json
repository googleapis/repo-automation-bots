{
  "name": "conventional-commit-lint",
  "version": "1.7.0",
  "description": "lint commit messages based on conventionalcommits.org",
  "private": true,
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "files": [
    "build/src"
  ],
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "conventional-commits",
    "lint"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "node ./build/src/server.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --node-option no-experimental-fetch build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/lint": "^17.1.0",
    "@google-automations/bot-config-utils": "^9.0.0",
    "@google-automations/issue-utils": "5.0.0",
    "gcf-utils": "^17.1.2",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.15",
    "@types/sinon": "^10.0.13",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "formidable": ">=2.1.3",
    "gts": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lru-cache": "^7.14.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "node-fetch": "^2.6.9",
    "sinon": "^15.0.0",
    "smee-client": "^1.2.3",
    "snap-shot-it": "^7.9.6",
    "typescript": "~4.9.0"
  },
  "overrides": {
    "tmp": "0.2.5"
  },
  "engines": {
    "node": ">= 18"
  }
}
