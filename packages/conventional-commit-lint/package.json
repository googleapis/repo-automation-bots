{
  "name": "conventional-commit-lint",
  "version": "1.3.1",
  "description": "lint commit messages based on conventionalcommits.org",
  "private": true,
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "conventional-commits",
    "lint"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/conventional-commit-lint.js",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal mocha --timeout=15000 --recursive ./build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/lint": "^8.0.0",
    "@octokit/rest": "^16.28.3",
    "gcf-utils": "^0.4.1",
    "probot": "^9.3.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/ioredis": "^4.0.13",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^5.2.7",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.6.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "gts": "^1.0.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "snap-shot-it": "^7.8.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
