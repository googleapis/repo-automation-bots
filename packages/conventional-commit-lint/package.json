{
  "name": "conventional-commit-lint",
  "version": "1.0.0",
  "description": "lint commit messages based on conventionalcommits.org",
  "author": "Google Inc.",
  "license": "ISC",
  "repository": "https://github.com//.git",
  "homepage": "https://github.com//",
  "bugs": "https://github.com///issues",
