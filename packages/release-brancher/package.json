  "devDependencies": {
    "@types/js-yaml": "^4.0.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.0",
    "@types/pino": "^6.3.6",
    "@types/sinon": "^10.0.0",
    "@types/yargs": "^16.0.0",
    "c8": "^7.3.5",
    "cross-env": "^7.0.0",
    "gts": "^3.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "sinon": "^11.0.0",
    "snap-shot-it": "^7.0.0",
    "typescript": "~4.3.0"
  },
  "config": {
    "snap-shot-it": {
      "sortSnapshots": true
    }
  },
  "engines": {
    "node": ">= 12.16.0"
  }
