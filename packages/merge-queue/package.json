{
    "name": "merge-queue",
    "version": "1.0.0",
    "description": "A bot for managing a merge queue for pull requests",
    "private": true,
    "author": "Google LLC.",
    "license": "Apache-2.0",
    "repository": "https://github.com/googleapis/repo-automation-bots.git",
    "homepage": "https://github.com/googleapis/repo-automation-bots",
    "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
    "main": "build/src/app.js",
    "keywords": [
        "probot",
        "github",
        "probot-app"
    ],
    "files": [
        "build/src"
    ],
    "scripts": {
        "compile": "tsc -p .",
        "pretest": "npm run compile && docker pull google/cloud-sdk",
        "start": "node ./build/src/server.js",
        "start-frontend": "node ./build/src/server-frontend.js",
        "start:local": "probot run ./build/src/local.js",
        "test": "cross-env LOG_LEVEL=fatal c8 mocha --exit build/test",
        "fix": "gts fix",
        "lint": "gts check"
    },
    "dependencies": {
        "@google-automations/label-utils": "^3.0.0",
        "@google-cloud/datastore": "^7.0.0",
        "gcf-utils": "^14.0.1",
        "google-datastore-emulator": "^6.0.0"
    },
    "devDependencies": {
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.7.13",
        "@types/sinon": "^10.0.13",
        "c8": "^7.12.0",
        "cross-env": "^7.0.3",
        "gts": "^4.0.0",
        "lru-cache": "^7.14.0",
        "mocha": "^10.0.0",
        "nock": "^13.2.9",
        "sinon": "^14.0.0",
        "smee-client": "^1.2.3",
        "typescript": "~4.8.1"
    },
    "engines": {
        "node": ">= 14"
    }
}
