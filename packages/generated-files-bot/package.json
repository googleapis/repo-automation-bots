{
  "name": "generated-files-bot",
  "version": "1.0.0",
  "description": "Detect modifications to templated files",
  "private": true,
  "author": "Google LLC.",
  "license": "Apache-2.0",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "main": "build/src/app.js",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "files": [
    "build/src"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "start": "probot run ./build/src/generated-files-bot.js",
    "start:local": "node ./build/src/local.js",
    "pretest": "npm run compile",
    "test": "cross-env LOG_LEVEL=fatal c8 mocha --exit build/test --timeout 10000",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "dependencies": {
    "gcf-utils": "^7.1.1",
    "js-yaml": "^4.0.0",
    "jsonpath": "^1.0.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.0",
    "@types/jsonpath": "^0.2.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.6.1",
    "c8": "^7.1.0",
    "cross-env": "^7.0.0",
    "gts": "^3.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "smee-client": "^1.1.0",
    "snap-shot-it": "^7.8.0",
    "typescript": "~4.1.0"
  },
  "engines": {
    "node": ">= 12.18.2"
  }
}
