    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.4",
    "@types/sinon": "^10.0.2",
    "c8": "^7.7.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "gts": "^3.1.0",
