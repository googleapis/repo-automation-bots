{
  "name": "@google-automations/bot-config-utils",
  "version": "8.0.0",
  "description": "Utilities for github bot config",
  "scripts": {
    "compile": "tsc -p .",
    "pretest": "npm run compile",
    "prepare": "npm run compile",
    "test": "cross-env NODE_ENV=test LOG_LEVEL=fatal c8 mocha --node-option no-experimental-fetch ./build/test",
    "test:snap": "SNAPSHOT_UPDATE=1 npm test",
    "system-test": "npm run pretest && cross-env LOG_LEVEL=fatal mocha ./build/test/integration",
    "fix": "gts fix",
    "lint": "gts check"
  },
  "main": "./build/src/bot-config-utils.js",
  "repository": "https://github.com/googleapis/repo-automation-bots.git",
  "homepage": "https://github.com/googleapis/repo-automation-bots",
  "bugs": "https://github.com/googleapis/repo-automation-bots/issues",
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "gcf-utils": "^16.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@octokit/types": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "@types/sinon": "^10.0.13",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "gts": "^4.0.0",
    "lru-cache": "^7.14.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "sinon": "^15.0.0",
    "snap-shot-it": "^7.9.6",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "Bot config",
    "Probot"
  ],
  "author": "Takashi Matsuo <tmatsuo@google.com>",
  "license": "Apache-2.0",
  "files": [
    "build/src"
  ]
}
